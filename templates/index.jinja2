{% extends "./base.jinja2" %}
{% from './_result.jinja2' import result_item with context %}
{% block content %}
    {% if error %}
        <p>{{ error }}</p>
    {% endif %}
    {% if  target != 'index'%}
        {% include "./form.jinja2" %}
    {% endif %} 
    {% if result %}
        {% for item in result %}
            {{ result_item(item.value, item.del_url, item.upd_url, item.dwld_url) }}
        {% endfor %}
    {% endif %}
{% endblock content %}